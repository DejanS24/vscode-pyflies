{
    "test": {
        "scope": "pyflies",
        "prefix": "test",
        "body": "test ${1:name} {\n\n${0}    |     |     |\n    | --- | --- |\n    |     |     |\n\n    fix -> cross()\n    exec-> circle(radius 20)\n}"
    },

    "repeat_with": {
        "scope": "pyflies",
        "prefix": "repeatwith",
        "body": "repeat {\n    ${1}\n}with\n${0}|     |     |\n| --- | --- |\n|     |     |\n"
    }
}
